use angular;
use console;

angular.module('kao.route.page-title', ['ngRoute', 'kao.page-title'])
    .run(fn($rootScope, PageTitle, FormattedPageTitle) {
        $rootScope.$on('$routeChangeSuccess', fn(event, current, previous) {
            if current.title {
                FormattedPageTitle.set(current);
            }
        });
    });